@model NewProject.Models.AddVehicle

@{
    ViewBag.Title = "vehicletabs";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/tabs.css" rel="stylesheet" />
<div class="card-body">
 
    <ul class="nav nav-tabs" id="myTab" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Add Vehicles</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">All Vehicles</a>
        </li>
        @*<li class="nav-item">
            <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">Menu 2</a>
        </li>*@
    </ul>
    <div class="tab-content pl-3 p-1" id="myTabContent">
        <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
            <h3>Add Vehicles</h3>
            <p> @Html.Action("Index")</p>
        </div>
        <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
            <h3>All Vehicles</h3>
            <p> @Html.Action( "Create")</p>
        </div>
    
    </div>


</div>







